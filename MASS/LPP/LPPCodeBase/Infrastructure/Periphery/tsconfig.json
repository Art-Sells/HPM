{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "types": [
      "hardhat",
      "@nomicfoundation/hardhat-ethers",
      "@nomicfoundation/hardhat-chai-matchers"
    ],
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": false
  },
  "include": ["hardhat.config.cjs", "scripts", "test", "typechain-types"]
}